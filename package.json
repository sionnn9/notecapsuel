{
  "name": "notecapsuel",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install-all": "npm install --prefix frontend/note-cap && npm install --prefix backend",
    "build": "npm run build --prefix frontend/note-cap",
    "start-backend": "npm run start --prefix backend",
    "start-frontend": "npm run start --prefix frontend/note-cap",
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sionnn9/notecapsuel.git"
  },
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
